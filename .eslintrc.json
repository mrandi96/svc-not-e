{
  "env": {
      "es6": true,
      "browser": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "no-useless-catch": ["off"],
      "camelcase": "warn",
      "no-underscore-dangle": ["error", { "allowAfterThis": true }],
      "comma-dangle": ["error", "never"]
  }
}